local Tab = Window:NewTab("Local Player")
local Section = Tab:NewSection("Speed + JumpPower")
Section:NewSlider("Walkspeed", "Walkspeed", 100, 24, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

Section:NewSlider("JumpSpeed", "JumpSpeed", 250, 65, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpSpeed = s
end)

local Tab = Window:NewTab("Settings")
local Section = Tab:NewSection("Settings")
Section:NewKeybind("Toggle UI", "Toggles the User Interface", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)
